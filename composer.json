{
  "name": "erekle/weather",
  "description": "Weather Api",
  "homepage": "https://github.com/erekle1/laravel-weather",
  "keywords": [
    "laravel-weather",
    "php-weather",
    "laravel"
  ],
  "authors": [
    {
      "name": "erekle",
      "email": "erekle.k1@gmail.com"
    }
  ],
  "require": {
    "php": ">=7",
    "illuminate/support": "5.5.*",
    "guzzlehttp/guzzle": "6.*",
    "nesbot/carbon": "^1.22"
  },
  "require-dev": {
    "mockery/mockery": "^0.9.4",
    "phpunit/phpunit": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "Erekle\\Weather\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Erekle\\Weather\\Test\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Erekle\\Weather\\WeatherServiceProvider"
      ],
      "aliases": {
        "Weather": "Erekle\\Weather\\Facades\\Weather"
      }
    }
  },
  "minimum-stability": "dev"

}
